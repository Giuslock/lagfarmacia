<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="665.0" prefWidth="816.0" styleClass="layout" stylesheets="@layout.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.univaq.oop.controller.FarmaciController">
   <children>
      <Label prefHeight="73.0" prefWidth="199.0" text="  Elenco Farmaci:">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button fx:id="aggiungiButton" mnemonicParsing="false" onAction="#aggiungiAction" onMouseClicked="#aggiungiAction" prefHeight="33.0" prefWidth="154.0" styleClass="Button" stylesheets="@loginButton.css" text="Nuovo farmaco ">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Separator opacity="0.0" prefHeight="12.0" prefWidth="700.0" />
      <TableView fx:id="elencoFarmaciTable" prefHeight="370.0" prefWidth="700.0">
         <columns>
            <TableColumn fx:id="codiceTableColumn" prefWidth="89.0" text="Codice" />
            <TableColumn fx:id="nomeTableColumn" prefWidth="93.0" text="Nome" />
            <TableColumn fx:id="statoTableColumn" prefWidth="118.0" text="Stato" />
            <TableColumn fx:id="minimumTableColumn" prefWidth="118.0" text="Minimum" />
            <TableColumn fx:id="quantityTableColumn1" prefWidth="118.0" text="ActualQuantity" />
            <TableColumn fx:id="azioniTableColumn" prefWidth="117.0" text="Azioni" />
         </columns>
      </TableView>
   </children>
</VBox>
